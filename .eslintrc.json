{
  "extends": "next/core-web-vitals",
  "rules": {
    // React
    // "react-refresh/only-export-components": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/prop-types": 0,
    "react/display-name": 0,

    // MUI
    "no-restricted-imports": [
